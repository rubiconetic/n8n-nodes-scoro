{
  "name": "n8n-nodes-scoro",
  "version": "0.1.1",
  "description": "n8n community node for the Scoro API",
  "license": "MIT",
  "homepage": "https://github.com/rubiconetic/n8n-nodes-scoro",
  "keywords": [
    "n8n-community-node-package",
    "scoro"
  ],
  "author": {
    "name": "Michael Rubi",
    "email": "michael@rubiconetic.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubiconetic/n8n-nodes-scoro.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "predev": "npx kill-port 5678",
    "dev": "n8n-node dev",
    "generate": "node scripts/generateResource.js",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ScoroApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Scoro/Scoro.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
